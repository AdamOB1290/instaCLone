{
  "extends": "stylelint-config-recommended",
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extends",
        "tailwind"
      ]
    }],
    "block-no-empty": null,
    "unit-whitelist": ["em", "rem", "s", "%", "px", "deg", "vw", "vh"],
    "unit-allowed-list":["em", "rem", "s", "%", "px", "deg","vw", "vh", "@import"]
  }
}